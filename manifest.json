{
  "manifest_version": 3,
  "name": "Insurance Claim Helper",
  "version": "0.1.0",
  "description": "A plugin to help capture and organize receipt/invoice information for insurance claims.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
